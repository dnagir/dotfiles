# http://docs.aws.amazon.com/cli/latest/userguide/cli-install-macos.html#awscli-install-osx-path
# Need correct Python version, installed with 'brew install python3'

aws_dir=~/Library/Python/3.6/bin
export AWS_CLI=$aws_dir/aws
export PATH=$aws_dir:$PATH


dotfile_aws_docker_login() { eval $(aws ecr get-login --no-include-email); }
# usage alias aws-docker-login dev-admin
alias aws-docker-login=dotfile_aws_docker_login

alias cf="aws cloudformation"
