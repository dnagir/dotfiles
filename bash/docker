alias d='docker'
alias dc='docker-compose'
alias docker-rma='docker rm -f $(docker ps -a -q)'
alias docker-rmia='docker rmi $(docker images -q)'

alias docker-stop-all='docker stop $(docker ps -q)'
alias docker-clean-images="docker images > /tmp/docker-images-cleanup && vim /tmp/docker-images-cleanup && cat /tmp/docker-images-cleanup | docker rmi -f \$(awk 'NR!=1{print $3}')"

