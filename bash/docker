alias d='docker'
alias dc='docker-compose'
alias drma='docker rm -f $(docker ps -a -q)'
alias drmia='docker rmi $(docker images -q)'

alias docker-clean-images="docker images > /tmp/docker-images-cleanup && vim /tmp/docker-images-cleanup && cat /tmp/docker-images-cleanup | docker rmi -f \$(awk 'NR!=1{print $3}')"

